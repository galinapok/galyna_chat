CREATE TABLE USERS(
   	ID INTEGER  PRIMARY KEY     AUTOINCREMENT,
   	NAME           TEXT    NOT NULL  UNIQUE,
   	EMAIL          CHAR(50)  NOT NULL  UNIQUE,     
  	PASSWORD       CHAR(50)
  
);
CREATE TABLE ROOMS(
   	ID INTEGER  PRIMARY KEY     AUTOINCREMENT,
   	NAME           TEXT    NOT NULL  UNIQUE  	
  
);